datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model EmailLog {
  body        String?
  createdAt   DateTime  @default(now())
  email       String?
  htmlMessage String?
  id          String    @id @default(cuid())
  sentAt      DateTime?
  subject     String?
  textMessage String?
  updatedAt   DateTime  @updatedAt
}
